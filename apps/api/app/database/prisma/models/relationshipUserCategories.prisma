model RelationshipUserCategories {
  user_id     String @db.Uuid /// ユーザーカテゴリID
  category_id Int /// ユーザーカテゴリ名

  created_at DateTime @default(now())
  updated_at DateTime @updatedAt()

  user     User                   @relation(fields: [user_id], references: [id], onUpdate: Cascade, onDelete: NoAction) /// ユーザーカテゴリID
  category RelationshipCategories @relation(fields: [category_id], references: [id], onUpdate: Cascade, onDelete: NoAction) /// ユーザーカテゴリ名

  @@id([user_id, category_id])
  @@map("relationship_user_categories")
}
